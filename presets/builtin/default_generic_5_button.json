{
  "id": "default_generic_5_button",
  "name": "Generic 5-Button Default",
  "version": "1.0",
  "mouseId": "generic_5_button",
  "author": "SlicerMouseMaster",
  "description": "Default bindings for generic 5-button mice",
  "mappings": {
    "middle": {
      "action": "view_reset_3d"
    },
    "back": {
      "action": "keyboard_shortcut",
      "parameters": {
        "key": "Left",
        "modifiers": ["ctrl"]
      }
    },
    "forward": {
      "action": "keyboard_shortcut",
      "parameters": {
        "key": "Right",
        "modifiers": ["ctrl"]
      }
    }
  },
  "contextMappings": {
    "SegmentEditor": {
      "back": {
        "action": "edit_undo"
      },
      "forward": {
        "action": "edit_redo"
      }
    },
    "Markups": {
      "back": {
        "action": "edit_undo"
      },
      "forward": {
        "action": "edit_redo"
      }
    },
    "VolumeRendering": {
      "back": {
        "action": "volumerendering_toggle"
      },
      "forward": {
        "action": "view_reset_3d"
      }
    }
  }
}
