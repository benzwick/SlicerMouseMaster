{
  "id": "default_mx_master_3s",
  "name": "MX Master 3S Default",
  "version": "1.0",
  "mouseId": "logitech_mx_master_3s",
  "author": "SlicerMouseMaster",
  "description": "Default bindings for Logitech MX Master 3S with Segment Editor optimizations",
  "mappings": {
    "middle": {
      "action": "view_reset_3d"
    },
    "back": {
      "action": "edit_undo"
    },
    "forward": {
      "action": "edit_redo"
    },
    "thumb": {
      "action": "view_toggle_crosshair"
    }
  },
  "contextMappings": {
    "SegmentEditor": {
      "back": {
        "action": "segment_previous"
      },
      "forward": {
        "action": "segment_next"
      },
      "thumb": {
        "action": "segment_editor_paint"
      }
    },
    "Markups": {
      "back": {
        "action": "markups_delete_point"
      },
      "thumb": {
        "action": "markups_place_fiducial"
      }
    },
    "VolumeRendering": {
      "thumb": {
        "action": "volumerendering_toggle"
      }
    }
  }
}
